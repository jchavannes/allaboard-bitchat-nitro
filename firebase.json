{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/login",
        "function": "login"
      },
      {
        "source": "/hclogin",
        "function": "hcLogin"
      },
      {
        "source": "/hcprofile",
        "function": "hcProfile"
      },
      {
        "source": "/hcsend",
        "function": "hcSendMessage"
      },
      {
        "source": "/hcsignops",
        "function": "hcSignOpReturnWithAIP"
      },
      {
        "source": "/hcencrypt",
        "function": "hcEncrypt"
      },
      {
        "source": "/hcdecrypt",
        "function": "hcDecrypt"
      },
    },
    {
      "source": "/baploadid",
      "function": "bapLoadID"
    },
    {
      "source": "**",
      "destination": "/index.html"
    }
  ]
},
"functions": {
  "ignore": [
    "node_modules",
    ".git",
    "firebase-debug.log",
    "firebase-debug.*.log"
  ]
}
}